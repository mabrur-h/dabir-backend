{
  "name": "uznotes-ai",
  "version": "1.0.0",
  "type": "module",
  "description": "AI Voice Notetaker Backend - Transcribe and summarize lectures with Gemini 2.5 Flash",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "worker": "tsx watch src/workers/index.ts",
    "worker:prod": "node dist/workers/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:seed": "tsx src/db/seed.ts",
    "db:studio": "drizzle-kit studio",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "transcription",
    "summarization",
    "gemini",
    "uzbek",
    "ai",
    "voice-notes"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/storage": "^7.14.0",
    "@google-cloud/vertexai": "^1.9.0",
    "@telegram-apps/init-data-node": "^2.0.10",
    "@tma.js/init-data-node": "^2.0.4",
    "@tus/gcs-store": "^2.0.0",
    "@tus/server": "^1.8.0",
    "@types/swagger-ui-express": "^4.1.8",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.30.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.2",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "fluent-ffmpeg": "^2.1.3",
    "google-auth-library": "^10.5.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pg": "^8.13.1",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "rate-limit-redis": "^4.3.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.16.0",
    "openapi-types": "^12.1.3",
    "prettier": "^3.4.2",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
